{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/token",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
            "Host": "project-wta-aut-api-svc",
            "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/token",
      "UpstreamHttpMethod": [ "Get","Post" ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 10,
        "Limit": 1
      }
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "192.168.49.2",
      "Port": 443,
      "Namespace": "project-wta",
      "Type": "KubernetesServiceDiscoveryProvider"
    },
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "HttpStatusCode": 429
    }
  },
  "DelegatingHandlers": [
    "ApiKeyHandler"
  ]
}